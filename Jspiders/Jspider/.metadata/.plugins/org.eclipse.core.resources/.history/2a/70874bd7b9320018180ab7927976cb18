import java.util.Scanner;

public class PatternRaj2 {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		print(n);
	}
		static void print(int n)
		{
		    int m = 1;
		    int i,j;
		    for( i=1; i<=n-1; i++)
		    {
		        if(i!=2)
		        {
		        	for(j=1; j<=n; j++){
		        		System.out.println(m);
		        		m++;
		        	}
		        	printf( "\n");
		        }else{
		        	m = m+n;
		        	for(j=1; j<=n; j++){
		        		printf(“%d ”, m);
		        		m++;
		        	}
		        	printf(“\n”);
		        }
		    }
		    for(i=n+1 ;i<=(n+n);i++){
		    	printf(“%d ”, i);
		    }
		    printf(“\n”);
		}
	}
}
